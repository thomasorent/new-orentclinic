# OpenAI API Configuration
# Copy this file to .env and add your actual API key
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Optional: Customize chatbot settings
REACT_APP_CHATBOT_THEME=light
REACT_APP_CHATBOT_POSITION=bottom-right
REACT_APP_CHATBOT_AUTO_OPEN=false

# WhatsApp Business API Configuration
# Get these from Meta Developer Console (https://developers.facebook.com/)
WHATSAPP_TOKEN=your_whatsapp_access_token_here
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id_here
WHATSAPP_VERIFY_TOKEN=your_custom_verify_token_here

# PostgreSQL Database Configuration
# Update these values for your database setup
DB_USER=postgres
DB_HOST=localhost
DB_NAME=orent_clinic
DB_PASSWORD=your_database_password_here
DB_PORT=5432 